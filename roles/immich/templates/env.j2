IMMICH_VERSION={{ immich_version }}
UPLOAD_LOCATION={{ immich_upload_path }}
TZ={{ immich_tz }}
IMMICH_TELEMETRY_INCLUDE=all

DB_HOSTNAME={{ immich_postgresql_host }}
DB_PORT={{ immich_postgresql_port }}
DB_USERNAME={{ immich_db_username }}
DB_PASSWORD={{ immich_db_password }}
DB_DATABASE_NAME={{ immich_db_database }}

REDIS_HOSTNAME={{ immich_redis_host }}
REDIS_PORT={{ immich_redis_port }}
REDIS_PASSWORD={{ immich_redis_password }}

{% if immich_oidc_enabled | default(false) | bool %}
IMMICH_CONFIG_FILE=/config/immich.json
IMMICH_TRUSTED_PROXIES={{ immich_trusted_proxies }}
{% endif %}
