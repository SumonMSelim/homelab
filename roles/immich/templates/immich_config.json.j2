{
  "oauth": {
    "enabled": true,
    "issuerUrl": "{{ immich_oidc_issuer_url }}",
    "clientId": "{{ immich_oidc_vault_secret.data.data.data.client_id }}",
    "clientSecret": "{{ immich_oidc_vault_secret.data.data.data.client_secret }}",
    "scope": "{{ immich_oidc_scope }}",
    "buttonText": "{{ immich_oidc_button_text }}",
    "autoRegister": {% if immich_oidc_auto_register %}true{% else %}false{% endif %},
    "autoLaunch": {% if immich_oidc_auto_launch %}true{% else %}false{% endif %},
    "signingAlgorithm": "RS256",
    "profileSigningAlgorithm": "none",
    "storageLabelClaim": "preferred_username",
    "roleClaim": "immich_role",
    "tokenEndpointAuthMethod": "client_secret_post"
  }
}
